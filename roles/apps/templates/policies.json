{
  "policies":{
    "Extensions": {
      "Locked": [
        "https://addons.mozilla.org/firefox/downloads/file/3551054/ublock_origin-latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/file/4206186/noscript-latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/file/4128403/bitwarden_password_manager-latest.xpi"
      ],
      "Install": [
	      "https://addons.mozilla.org/firefox/downloads/file/3756025/videospeed-latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/file/4223104/darkreader-latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/file/3535009/redirector-latest.xpi",

	      "https://addons.mozilla.org/firefox/downloads/file/3914147/matte_black_violet-latest.xpi"
      ]
    },
    "DisableBuiltinPDFViewer": true,
    "DisplayMenuBar": "never",
    "DontCheckDefaultBrowser": true,
    "DisplayBookmarksToolbar": "newtab",
    "DisableFeedbackCommands": true,
    "DisableFirefoxScreenshots": true,
    "DisableForgetButton": true,
    "DisableFormHistory": true,
    "DisablePrivateBrowsing": true,
    "DisableProfileRefresh": true,
    "DisableSafeMode": true,
    "DisableSecurityBypass": {
      "InvalidCertificate": true,
      "SafeBrowsing": true
    },
    "DisableSystemAddonUpdate": true,
    "HardwareAcceleration": true,
    "ExtensionUpdate": true,
    "DisableTelemetry": true,
    "DisableFirefoxStudies": true,
    "EnableTrackingProtection": {
      "Value": true,
      "Locked": false,
      "Cryptomining": true,
      "Fingerprinting": true,
      "EmailTracking": true,
      "Exceptions": []
    },
    "FirefoxHome": {
      "Highlights": false,
      "SponsoredTopSites": false,
      "Pocket": false,
      "Search": true,
      "SponsoredPocket": false,
      "Snippets": false,
      "TopSites": true,
      "Locked": true
    },
    "FirefoxSuggest": {
      "WebSuggestions": true,
      "SponsoredSuggestions": false,
      "ImproveSuggest": false,
      "Locked": true
    },
    "OfferToSaveLogins": false,
    "PasswordManagerEnabled": false,
    "DisablePasswordReveal": true,
    "Preferences": {
      "browser.search.update": true,
      "media.gmp-gmpopenh264.enabled": true,
      "media.gmp-widevinecdm.enabled": true,
      "browser.urlbar.suggest.bookmark": true,
      "browser.urlbar.suggest.history": true,
      "browser.urlbar.suggest.openpage": true,
      "places.history.enabled": true
    },
    "DefaultDownloadDirectory": "${home}",
    "DownloadDirectory": "${home}",
    "SearchSuggestEnabled": true,
    "SearchBar": "unified",
    "OverrideFirstRunPage": "",
    "DisableMasterPasswordCreation": true,
    "DisablePocket": true,
    "DisableProfileImport": true,
    "DisableSetDesktopBackground": true,
    "SearchEngines": {
      "Default": "PrivateSearch",
      "Add": [
        {
          "Name": "PrivateSearch",
          "URLTemplate": "https://duckduckgo.com/?q={searchTerms}&t=h_&kp=1&ia=web",
          "Method": "GET",
          "IconURL": "https://duckduckgo.com/favicon.ico",
          "Alias": "Ducky",
          "Description": "Duck Duck Goose"
        }
      ],
      "Remove": [
        "Google",
        "Amazon.com",
        "Bing",
        "eBay",
        "Wikipedia (en)"
      ]
    },
    "UserMessaging": {
      "WhatsNew": false,
      "ExtensionRecommendations": false,
      "FeatureRecommendations": false,
      "UrlbarInterventions": false,
      "SkipOnboarding": true,
      "MoreFromMozilla": false,
      "Locked": true
    }
  }
}
